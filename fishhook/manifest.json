{
  "name": "Fishhook",
  "description": "Notice suspicious things in gmail",
  "version": "0.1",
  "manifest_version": 2,
  "permissions": [
    
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [ "*://mail.google.com/*" ],
      "js": [ "jquery-1.11.3.min.js", "gmail.js" ],
      "css":  ["gmail.css"]
    }
  ]
}